{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "node index.js",
    "lint": "eslint . --ext .js",
    "test": "jest --testPathPattern=tests/services --runInBand",
    "test:all": "jest --runInBand",
    "backfill:results": "node scripts/backfill_results.js"
  },
  "dependencies": {
    "asn1.js": "^5.4.1",
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "geoip-lite": "^1.4.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "redis": "^4.6.12",
    "speakeasy": "^2.0.0",
    "ssh2": "^1.15.0"
  },
  "devDependencies": {
    "eslint": "^8.46.0",
    "jest": "^29.6.1"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      },
      "services/**": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    }
  }
}
